<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.projectmanager.GUIs.logInController">
   <center>
      <HBox prefHeight="0.0" prefWidth="614.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="Employee ID" />
            <TextField id="employeeIDField" fx:id="employeeIDField" prefHeight="29.0" prefWidth="202.0" />
            <Separator opacity="0.0" orientation="VERTICAL" prefHeight="29.0" prefWidth="59.0" />
            <Label text="Password" />
            <PasswordField id="passwordField" fx:id="passwordField" prefHeight="29.0" prefWidth="198.0" />
         </children>
      </HBox>
   </center>
   <top>
      <ImageView fitHeight="427.0" fitWidth="671.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER">
         <image>
            <Image url="@Logo.png" />
         </image></ImageView>
   </top>
   <right>
      <Button mnemonicParsing="false" onAction="#logInButtonClick" text="Log In" BorderPane.alignment="CENTER" />
   </right>
   <bottom>
      <Label id="warningLabel" fx:id="warningLabel" prefHeight="43.0" prefWidth="224.0" textFill="#e1730c" BorderPane.alignment="CENTER">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
   </bottom>
</BorderPane>
